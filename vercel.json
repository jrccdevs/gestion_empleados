{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["node_modules/mysql2/**", "node_modules/sequelize/**"] 
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ]
  }