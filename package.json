{
  "name": "empleados-api",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "npx sequelize-cli db:migrate --config src/config/config.js",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --config src/config/config.js",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all --config src/config/config.js",
    "seed": "npx sequelize-cli db:seed --config src/config/config.js",
    "seed:all": "npx sequelize-cli db:seed:all --config src/config/config.js",
    "seed:undo": "npx sequelize-cli db:seed:undo --config src/config/config.js",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all --config src/config/config.js",
    "migration:create": "npx sequelize-cli migration:generate --config src/config/config.js --name",
    "seeder:create": "npx sequelize-cli seed:generate --config src/config/config.js --name"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.1",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@vercel/node": "^5.2.2",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3"
  }
}
